#include <stdio.h>
#include <string.h>

int main() {
    char haystack[100], needle[50];
    
    printf("Enter the main string (haystack): ");
    gets(haystack);
    
    printf("Enter the substring to search (needle): ");
    gets(needle);

    if (strstr(haystack, needle) != NULL)
        printf("Substring found!\n");
    else
        printf("Substring not found!\n");

    return 0;
}
