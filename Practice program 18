#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    int arr[n];
    for (int i = 0; i < n; i++) scanf("%d", &arr[i]);

    if (n >= 5)
        printf("5th element = %d\n", arr[4]);  // index 4 = 5th element
    else
        printf("Array has less than 5 elements\n");

    return 0;
}
