#include <stdio.h>

int main() {
    int n, a = 0, b = 1, c, sum = 0;
    scanf("%d", &n);

    if (n >= 1) sum += a;
    if (n >= 2) sum += b;

    for (int i = 3; i <= n; i++) {
        c = a + b;
        sum += c;
        a = b;
        b = c;
    }

    printf("Sum of Fibonacci series up to %d terms = %d\n", n, sum);
    return 0;
}
