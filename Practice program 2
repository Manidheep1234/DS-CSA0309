#include <stdio.h>
#include <string.h>
#include <limits.h>
#include <ctype.h>
#include <stdlib.h>

int isValidInteger(char *s) {
    int i = 0;
    if (s[0] == '-' || s[0] == '+') i = 1;
    if (s[i] == '\0') return 0;
    for (; s[i]; i++) if (!isdigit(s[i])) return 0;
    long val = atol(s);
    if (val > INT_MAX || val < INT_MIN) return 0;
    return 1;
}

int reverse(int x) {
    long rev = 0;
    while (x != 0) {
        rev = rev * 10 + x % 10;
        x /= 10;
        if (rev > INT_MAX || rev < INT_MIN) return 0;
    }
    return (int)rev;
}

int main() {
    char s[50];
    scanf("%s", s);
    if (!isValidInteger(s)) {
        printf("Invalid Input\n");
        return 0;
    }
    int n = atoi(s);
    printf("%d\n", reverse(n));
    return 0;
}
