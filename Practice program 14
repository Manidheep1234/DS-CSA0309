#include <stdio.h>
#include <limits.h>

#define N 4

int tsp(int city, int visited, int dist[N][N], int n) {
    if (visited == (1<<n) - 1) return dist[city][0]; // return to start

    int ans = INT_MAX;
    for (int i = 0; i < n; i++) {
        if (!(visited & (1<<i))) {
            int temp = dist[city][i] + tsp(i, visited | (1<<i), dist, n);
            if (temp < ans) ans = temp;
        }
    }
    return ans;
}

int main() {
    int dist[N][N] = {
        {0, 10, 15, 20},
        {10, 0, 35, 25},
        {15, 35, 0, 30},
        {20, 25, 30, 0}
    };

    int result = tsp(0, 1, dist, N);
    printf("Minimum cost of TSP tour: %d\n", result);
    return 0;
}
